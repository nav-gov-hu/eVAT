	<xs:simpleType name="DeclarationSchemaType">
		<xs:annotation>
			<xs:documentation xml:lang="hu">A bevallás sémájának típusa</xs:documentation>
			<xs:documentation xml:lang="en">Type of declaration schema</xs:documentation>
		</xs:annotation>
