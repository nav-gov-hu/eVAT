					<xs:documentation xml:lang="en">Register A</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="B_REGISTER">
				<xs:annotation>
					<xs:documentation xml:lang="hu">B gyűjtő</xs:documentation>
					<xs:documentation xml:lang="en">Register B</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="C_REGISTER">
				<xs:annotation>
					<xs:documentation xml:lang="hu">C gyűjtő</xs:documentation>
					<xs:documentation xml:lang="en">Register C</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="D_REGISTER">
				<xs:annotation>
					<xs:documentation xml:lang="hu">D gyűjtő</xs:documentation>
					<xs:documentation xml:lang="en">Register D</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="E_REGISTER">
				<xs:annotation>
					<xs:documentation xml:lang="hu">E gyűjtő</xs:documentation>
					<xs:documentation xml:lang="en">Register E</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
