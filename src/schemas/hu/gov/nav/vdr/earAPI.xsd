				<xs:annotation>
					<xs:documentation xml:lang="hu">Importőr</xs:documentation>
					<xs:documentation xml:lang="en">Importer</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="INDIRECT_REPRESENTATIVE">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Közvetett vámjogi képviselő</xs:documentation>
					<xs:documentation xml:lang="en">Indirect customs representative</xs:documentation>
