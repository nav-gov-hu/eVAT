<xs:schema xmlns="http://schemas.nav.gov.hu/EAR/1.0/data" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:common="http://schemas.nav.gov.hu/NTCA/1.0/common" xmlns:earbase="http://schemas.nav.gov.hu/EAR/1.0/base" targetNamespace="http://schemas.nav.gov.hu/EAR/1.0/data" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:import namespace="http://schemas.nav.gov.hu/NTCA/1.0/common" schemaLocation="../../NTCA/1.0/common.xsd"/>
	<xs:import namespace="http://schemas.nav.gov.hu/EAR/1.0/base" schemaLocation="earBase.xsd"/>
	<xs:simpleType name="AttachmentCategoryType">
		<xs:annotation>
			<xs:documentation xml:lang="hu">Csatolmány típus</xs:documentation>
			<xs:documentation xml:lang="en">Attachment type</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="FACTORING_CONTRACT">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Faktorálási szerződés</xs:documentation>
					<xs:documentation xml:lang="en">Factoring contract</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FactoringTaxCodeType">
		<xs:annotation>
			<xs:documentation xml:lang="hu">Faktorálási szerződéshez kapcsolódó adókód</xs:documentation>
			<xs:documentation xml:lang="en">Tax code related to the factoring contract</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="VAT_104">
				<xs:annotation>
					<xs:documentation xml:lang="hu">104-es adónemkód ÁFÁ-hoz</xs:documentation>
					<xs:documentation xml:lang="en">104 tax code for VAT</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SELF_CHECK_ALLOWANCE_215">
				<xs:annotation>
					<xs:documentation xml:lang="hu">215-ös adókód önellenőrzési pótlékhoz</xs:documentation>
					<xs:documentation xml:lang="en">215 tax code for self check allowance</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="OutOfTurnDeclarationCodeType">
		<xs:annotation>
			<xs:documentation xml:lang="hu">Soron kívüli bevallás kódja</xs:documentation>
			<xs:documentation xml:lang="en">Out-of-turn declaration's code</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="PRE_COMPANY_PERIOD_CLOSURE">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Előtársasági időszak lezárása</xs:documentation>
					<xs:documentation xml:lang="en">Closing of the pre-company period</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FOREIGN_CURRENCY_CHANGE">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Devizanem váltása</xs:documentation>
					<xs:documentation xml:lang="en">Change currency</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OTHER_INCORPORATION_CHANGE">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Egyéb gazdálkodó átalakulása</xs:documentation>
					<xs:documentation xml:lang="en">Transformation of other incorporation</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PRE_COMPANY_PERIOD_DECLARATION">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Előtársasági időszakot tartalmazó teljes bevallási időszak</xs:documentation>
					<xs:documentation xml:lang="en">Complete declaration period including pre-company period</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TAX_WAREHOUSE_OUT_OF_TURN_DECLARATION">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Adóraktár képviselője által beadott soron kívüli bevallás az ÁFA törvény 89/A.§ szerint</xs:documentation>
					<xs:documentation xml:lang="en">Out-of-turn declaration submitted by a tax warehouse representative according to § 89/A of the VAT Act</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PartnerStatusType">
		<xs:annotation>
			<xs:documentation xml:lang="hu">Partner áfa szerinti státusza</xs:documentation>
			<xs:documentation xml:lang="en">Partner's VAT-related status</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="NOT_AVAILABLE">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Nem ismert az ügyviteli program számára</xs:documentation>
					<xs:documentation xml:lang="en">Not available for the management software</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PRIVATE_PERSON">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Nem áfaalany, külföldi vagy belföldi természetes személy</xs:documentation>
					<xs:documentation xml:lang="en">Non-VAT subject (domestic or foreign) natural person</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DOMESTIC">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Belföldi áfaalany</xs:documentation>
					<xs:documentation xml:lang="en">Domestic VAT subject</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OTHER">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Egyéb (belföldi nem áfaalany, nem természetes személy, külföldi áfaalany és külföldi nem áfaalany, nem természetes személy)</xs:documentation>
					<xs:documentation xml:lang="en">Other (domestic non-VAT subject, non-natural person, foreign VAT subject and foreign non-VAT subject, non-natural person)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PositionTypeType">
		<xs:annotation>
			<xs:documentation xml:lang="hu">Áfa pozíció jelölése</xs:documentation>
			<xs:documentation xml:lang="en">VAT position type</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="PAYABLE">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Fizetendő áfa</xs:documentation>
					<xs:documentation xml:lang="en">Payable VAT</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DEDUCTIBLE">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Levonható áfa</xs:documentation>
					<xs:documentation xml:lang="en">Deductible VAT</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OTHER">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Egyéb</xs:documentation>
					<xs:documentation xml:lang="en">Other</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ReturnDecisionType">
		<xs:annotation>
			<xs:documentation xml:lang="hu">Visszautalási döntés</xs:documentation>
			<xs:documentation xml:lang="en">Return decision</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="NO_RETURN">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Nem kér kiutalást</xs:documentation>
					<xs:documentation xml:lang="en">No request for return</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FULL_RETURN">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Teljes összeg kiutalása</xs:documentation>
					<xs:documentation xml:lang="en">Return of the full amount</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TAX_ACCOUNT_TRANSFER">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Adószámlára átvezetési kérelem</xs:documentation>
					<xs:documentation xml:lang="en">Request for transfer to a tax account</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SheetFieldNameType">
		<xs:annotation>
			<xs:documentation xml:lang="hu">Melléklapon szereplő BEVFELD mező azonosítója</xs:documentation>
			<xs:documentation xml:lang="en">BEVFELD field identifier on the sheet</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="13"/>
			<xs:minLength value="4"/>
			<xs:pattern value="[A-Z0-9]{4,13}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SheetNameType">
		<xs:annotation>
			<xs:documentation xml:lang="hu">Melléklap neve</xs:documentation>
			<xs:documentation xml:lang="en">Name of the sheet</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="VAT_SHEET_2">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Állatbetegséggel összefüggő járványügyi intézkedés miatti kártalanítás okán fizetési halasztás kérése</xs:documentation>
					<xs:documentation xml:lang="en">Request for payment deferral because of compensation due to epidemiological measures related to animal diseases</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="VAT_SHEET_6">
				<xs:annotation>
					<xs:documentation xml:lang="hu">A behajthatatlan követelés elszámolásával összefüggő nyilatkozatok</xs:documentation>
					<xs:documentation xml:lang="en">Statements relating to the settlement of an irrecoverable debt</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="VAT_SHEET_7">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Az Áfa tv. 6/A., 6/B. és a 6/C. számú mellékletei szerint a fordított adózás keretében történt értékesítésre vonatkozó nyilatkozat</xs:documentation>
					<xs:documentation xml:lang="en">Statement for reverse charge sales according to Annexes 6/A, 6/B and 6/C of the VAT Act</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="VAT_SHEET_8">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Az Áfa tv. 6/A., 6/B. és a 6/C. számú mellékletei szerint a fordított adózás keretében történt beszerzésre vonatkozó nyilatkozat</xs:documentation>
					<xs:documentation xml:lang="en">Statement for reverse charge purchases according to Annexes 6/A, 6/B and 6/C of the VAT Act</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="VAT_SHEET_9">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Az Áfa tv. 4/A. számú melléklet II. pont szerinti adatszolgáltatás a személygépkocsi alvázszámáról</xs:documentation>
					<xs:documentation xml:lang="en">Data report on the chassis number of passenger car according to Annex 4/A, point II of the VAT Act</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="VAT_SHEET_A88">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Adatszolgáltatás új közlekedési eszköz értékesítéséről</xs:documentation>
					<xs:documentation xml:lang="en">Data report on the sale of new means of transport</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="VAT_SHEET_170">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Átvezetési és kiutalási kérelem a pénzforgalmat lebonyolító bizonylatokhoz</xs:documentation>
					<xs:documentation xml:lang="en">Transfer and assignment request for receipts handling cash flow</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="VAT_SHEET_4">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Önellenőrzési melléklet</xs:documentation>
					<xs:documentation xml:lang="en">Self-audit attachment</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="VAT_SHEET_EUNY">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Nyilatkozat az önellenőrzés indokáról, ha az adókötelezettséget megállapító jogszabály alaptörvény-ellenes vagy az Európai Unió kötelező jogi aktusába ütközik</xs:documentation>
					<xs:documentation xml:lang="en">Statement on the reason for the self-audit, if the legislation establishing tax liability is unconstitutional or conflicts with a mandatory legal act of the European Union</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SheetPageCountType">
		<xs:annotation>
			<xs:documentation xml:lang="hu">Melléklap oldalainak száma</xs:documentation>
			<xs:documentation xml:lang="en">Number of pages of a sheet</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:totalDigits value="3"/>
			<xs:minInclusive value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="VpidType">
		<xs:annotation>
			<xs:documentation xml:lang="hu">VPID szám</xs:documentation>
			<xs:documentation xml:lang="en">EORI number</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="12"/>
			<xs:pattern value="[0-9]{12}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SourceDocumentTypeType">
		<xs:annotation>
			<xs:documentation xml:lang="hu">Forrás dokumentum bizonylat típusa</xs:documentation>
			<xs:documentation xml:lang="en">Type of the source document</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="INVOICE">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Számla</xs:documentation>
					<xs:documentation xml:lang="en">Invoice</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RECEIPT">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Nyugta</xs:documentation>
					<xs:documentation xml:lang="en">Receipt</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CUSTOMS_DECLARATION">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Vámáru határozat</xs:documentation>
					<xs:documentation xml:lang="en">Customs declaration</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OTHER">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Egyéb bizonylat</xs:documentation>
					<xs:documentation xml:lang="en">Other document</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TaxMonetaryType">
		<xs:annotation>
			<xs:documentation xml:lang="hu">Adó összeg típusa</xs:documentation>
			<xs:documentation xml:lang="en">Tax amount type</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="18"/>
			<xs:fractionDigits value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TaxpayerStatusCodeType">
		<xs:annotation>
			<xs:documentation xml:lang="hu">Adózói státusz kódja</xs:documentation>
			<xs:documentation xml:lang="en">Taxpayer status code</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="CODE_1">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Közösségi adószámmal rendelkező, áfaalanynak nem minősülő adófizetésre kötelezett jogi személy</xs:documentation>
					<xs:documentation xml:lang="en">Legal person with Community tax number who is not a VAT subject but obligated to pay VAT</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CODE_3">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Közösségi adószámmal nem rendelkező, áfaalanynak nem minősülő természetes személy</xs:documentation>
					<xs:documentation xml:lang="en">Natural person without Community tax number who is not a VAT subject</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CODE_4">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Közösségi adószámmal nem rendelkező, áfaalanynak nem minősülő egyéb szervezet</xs:documentation>
					<xs:documentation xml:lang="en">Other organization without Community tax number that is not a VAT subject</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CODE_5">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Közösségi adószámmal nem rendelkező, áfaalanynak nem minősülő jogi személy</xs:documentation>
					<xs:documentation xml:lang="en">Legal person without Community tax number who is not a VAT subject</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TaxRateType">
		<xs:annotation>
			<xs:documentation xml:lang="hu">Hányados típusa</xs:documentation>
			<xs:documentation xml:lang="en">Rate type</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:fractionDigits value="4"/>
			<xs:totalDigits value="5"/>
			<xs:minExclusive value="0"/>
			<xs:maxExclusive value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="AdditionalAttachmentInfoType">
		<xs:annotation>
			<xs:documentation xml:lang="hu">Csatolmány egyéb adatai</xs:documentation>
			<xs:documentation xml:lang="en">Additional data of the attachment</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="factoringContractData" type="FactoringContractDataType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Faktorálási szerződés adatai</xs:documentation>
					<xs:documentation xml:lang="en">Factoring contract data</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="AgriculturalCompensationPremiumType">
		<xs:annotation>
			<xs:documentation xml:lang="hu">Mezőgazdasági kompenzációs felár</xs:documentation>
			<xs:documentation xml:lang="en">Agricultural compensation premium</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="sevenPercentCount" type="earbase:BevfeldThreeDigitNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="hu">7%-os mértékű kompenzáció darabszáma</xs:documentation>
					<xs:documentation xml:lang="en">Count of compensations at 7%</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="twelvePercentCount" type="earbase:BevfeldThreeDigitNumberType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="hu">12%-os mértékű kompenzáció darabszáma</xs:documentation>
					<xs:documentation xml:lang="en">Count of compensations at 12%</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AttachmentType">
		<xs:annotation>
			<xs:documentation xml:lang="hu">Csatolmányok</xs:documentation>
			<xs:documentation xml:lang="en">Attachments</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="claimCheckId" type="common:EntityIdType">
				<xs:annotation>
					<xs:documentation xml:lang="hu">A melléklet feltöltésekor kapott azonosító</xs:documentation>
					<xs:documentation xml:lang="en">Identifier received when uploading the attachment</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="attachmentCategory" type="AttachmentCategoryType">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Csatolmány típus</xs:documentation>
					<xs:documentation xml:lang="en">Attachment type</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="additionalAttachmentInfo" type="AdditionalAttachmentInfoType">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Csatolmány egyéb adatai</xs:documentation>
					<xs:documentation xml:lang="en">Additional data of the attachment</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DeclarationStatementsType">
		<xs:annotation>
			<xs:documentation xml:lang="hu">Nyilatkozatok</xs:documentation>
			<xs:documentation xml:lang="en">Statements</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="returnDecision" type="ReturnDecisionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Visszautalási döntés</xs:documentation>
					<xs:documentation xml:lang="en">Return decision</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taxpayerStatusCode" type="TaxpayerStatusCodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Adózói státusz</xs:documentation>
					<xs:documentation xml:lang="en">Taxpayer status</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="procedureStatements" type="ProcedureStatementsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Eljárási nyilatkozatok</xs:documentation>
					<xs:documentation xml:lang="en">Statements of procedures</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="returnStatements" type="ReturnStatementsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="hu">Visszautalási nyilatkozatok</xs:documentation>
					<xs:documentation xml:lang="en">Return statements</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DeclarationSummaryType">
		<xs:annotation>
			<xs:documentation xml:lang="hu">A bevallás összegző adatai</xs:documentation>
			<xs:documentation xml:lang="en">Summary data of the declaration</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="SummaryType">
				<xs:sequence>
