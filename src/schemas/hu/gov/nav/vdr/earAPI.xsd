	<xs:simpleType name="SoftwareIdType">
		<xs:annotation>
			<xs:documentation xml:lang="hu">Az ügyviteli program azonosítója</xs:documentation>
			<xs:documentation xml:lang="en">Identifier of the management software</xs:documentation>
		</xs:annotation>
		<xs:restriction base="common:AtomicStringType32">
			<xs:length value="18"/>
			<xs:pattern value="[0-9A-Z\-]{18}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SoftwareOperationType">
